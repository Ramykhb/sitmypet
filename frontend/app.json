{
  "expo": {
    "name": "sitmypet",
    "slug": "sitmypet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SitMyPet-iOS-Default.png",
    "scheme": "sitmypet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.sitmypet",
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/SitMyPet-iOS-Default.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64eaab78-d299-48ed-8297-0dfac24c1091"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/64eaab78-d299-48ed-8297-0dfac24c1091"
    }
  }
}
